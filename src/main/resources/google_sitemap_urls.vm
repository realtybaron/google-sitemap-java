<?xml version="1.0" encoding="UTF-8"?>

<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    #foreach( $entry in $entries )
        <url>
            <loc>$entry.url</loc>
            #if ($entry.lastModified)
                <lastmod>$entry.lastModifiedAsString</lastmod>
            #end
        </url>
    #end
</urlset>